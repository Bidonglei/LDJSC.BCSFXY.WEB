<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sunmnet.bigdata.systemManage.dao.diagnosisTeaching.SunmnetAcademyInfoDao" >
    <select id="queryStudentDepartment" resultType="com.sunmnet.bigdata.systemManage.base.systemManage.model.QueryStudentDepartmentModel">
        SELECT DISTINCT
            `bdi`.`code` AS `departmentCode`,
            `bdi`.`name` AS `departmentName`
        FROM
            `basic_department_info` `bdi`
        WHERE
            `bdi`.`status` = 0
            AND `bdi`.`type` = 0
        order by code asc
    </select>
</mapper>
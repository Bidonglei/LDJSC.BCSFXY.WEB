    <!--查询用户登录的权限菜单信息服务间调用根据用户编号和系统编号  2018-04-12 wm -->
	<select id="findUserRoleMenuInfo" resultMap="BaseResultMapMenu" parameterType="com.sunmnet.bigdata.systemManage.base.systemManage.model.QueryUserRoleMenuInfoRemoteRequestModel" >
       
       select  smi.menu_no,smi.menu_name,smi.menu_url,smi.menu_level,smi.system_no,smi.parent_menu_no,smi.data_department,smi.data,smi.judge from sunmnet_menu_info smi

 		RIGHT JOIN (select DISTINCT(srmi.menu_no)  from sunmnet_role_menu_info srmi
					where srmi.role_no in (select suri.role_no from sunmnet_user_role_info suri 
								where  suri.user_no = #{userNo,jdbcType=VARCHAR})) temp on  smi.menu_no=temp.menu_no
		where smi.system_no =#{systemNo,jdbcType=VARCHAR} order by CONVERT(smi.menu_sort_no,SIGNED) asc
       
    </select>


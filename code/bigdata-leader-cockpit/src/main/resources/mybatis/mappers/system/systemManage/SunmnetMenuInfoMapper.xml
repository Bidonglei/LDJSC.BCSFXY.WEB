    <!--查询用户拥有的系统权限  2018-07-31 wm-->
	<select id="queryUserSystemNo" resultType="com.sunmnet.bigdata.systemManage.base.systemManage.model.QueryUserSystemNoModel" parameterType="java.lang.String" >
        select ssi.system_no systemNo, ssi.system_name systemName, ssi.system_url systemUrl 
        	from sunmnet_system_info ssi,
				(select DISTINCT(smi.system_no) 
					from sunmnet_menu_info smi,
						 (select DISTINCT(srmi.menu_no)  from sunmnet_role_menu_info srmi 
							where srmi.role_no in (select suri.role_no from sunmnet_user_role_info suri 
								where  suri.user_no = #{userNo,jdbcType=VARCHAR}) ) temp 
					where temp.menu_no = smi.menu_no) sys 
			where sys.system_no=ssi.system_no
    </select>
    
    
    


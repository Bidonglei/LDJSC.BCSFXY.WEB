	<!--查询角色授权的菜单信息,和角色已经拥有的角色 2018-04-08 wm -->
	<select id="findSetRoleMenuInfo" resultMap="BaseResultMapRoleMenu" parameterType="com.sunmnet.bigdata.systemManage.base.systemManage.model.QueryRoleMenuInfoRequestModel" >
        select smi.parent_menu_no,smi.menu_no,smi.menu_name,smi.menu_level,smi.system_no, srmi.role_no

        	from sunmnet_menu_info smi 

 		LEFT JOIN (select * from sunmnet_role_menu_info srmi where srmi.role_no=#{roleNo,jdbcType=VARCHAR} ) srmi on  smi.menu_no=srmi.menu_no
		where smi.system_no !=#{systemNo,jdbcType=VARCHAR}
		order by CONVERT(menu_sort_no,SIGNED) asc 
        
    </select>

